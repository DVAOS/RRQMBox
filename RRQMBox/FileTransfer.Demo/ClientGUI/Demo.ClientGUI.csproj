<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Font\iconfont.ttf" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="RRQMCore" Version="1.2.18" />
    <PackageReference Include="RRQMSkin" Version="2.1.2" />
    <PackageReference Include="RRQMSocket" Version="4.0.12" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Font\iconfont.ttf">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
